						</execution>
					</executions>
					<dependencies>
						<dependency>
							<groupId>org.jasig.mojo.jspc</groupId>
							<artifactId>jspc-compiler-tomcat7</artifactId>
							<version>2.0.2</version>
						</dependency>
					</dependencies>
				</plugin>
